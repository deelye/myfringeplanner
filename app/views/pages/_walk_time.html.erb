<% if @planners[index + 1] != nil %>
  <% journey_info =  planner.calc_distance(@planners, index) %>
  <% if @planners[index + 1].performance.start < planner.walk_end(journey_info) %>
    <div class="planner-card walk-notif walk-red" style=" height: <%= planner.walk_duration(journey_info) %>; width: 100%">
  <% else %>
    <div class="planner-card walk-notif" style=" height: <%= planner.walk_duration(journey_info) %>; width: 100%">
  <% end %>
    <p><i class="fas fa-walking fa-fw"></i><%= (journey_info["routes"].first["duration"] / 60).ceil %> minute walk to <%= @planners[ index + 1 ].performance.show.venue.name %></p>
  </div>
<% end %>
