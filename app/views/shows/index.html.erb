
<div class="container">
    <%= form_tag shows_path, method: :get, class: "form-inline form-search d-flex" do %>
      <%= text_field_tag :query,
        params[:query],
        class: 'form-control',
        placeholder: 'Find a Show'
        %>
      <%= submit_tag 'Search', class: 'btn btn-success ml-2 px-3'%>
    <% end %>
  </div>

